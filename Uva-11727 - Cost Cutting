#include <stdio.h>

int main()
{
    int a,b,c,max,min,mid;
    int i,t;
    
    scanf("%d",&t);
    
    if(t<20){        
        
        for(i=0;i<t;i++){
            
            scanf("\n%d %d %d",&a,&b,&c);
            
            if(a>0 && b>0 && c>0){
                
                if(a>=1000 && a<=10000 && b>=1000 && b<=10000 && c>=1000 && c<=10000){
                
                    if(a>b && a>c){
                        max=a;
                    }else if(b>c && b>a ){
                        max=b;
                    }else{
                        max=c;
                    }
       
                    if(a<b && a<c){
                        min=a;
                    }else if(b<c && b<a ){
                        min=b;
                    }else{
                        min=c;
                    }
    
                    if(a<max && a>min){
                        mid=a;
                    }else if( b<max && b>min){
                        mid=b;
                    }else{
                        mid=c;
                    }
                }
            }
            
            printf("Case %d: %d\n",i+1,mid);
        }   
    }

    return 0;
}





