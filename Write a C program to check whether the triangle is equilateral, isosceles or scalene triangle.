#include <stdio.h>
int main()  
{  
    int a, b,c;
    
    printf("Check whether the triangle is equilateral, isosceles or scalene triangle. \n");
    printf("\nEnter First side: ");

    scanf("%d",&a);

    printf("\nEnter Second side: ");
    scanf("%d",&b);

    printf("\nEnter Third side: ");

    scanf("%d",&c);
    
    
    if(a!=0 && b!=0 && c!=0){
        
        if(a==b && b==c && a==c){
            printf("Equilateral Triangle");
        }else if(a==b || b==c || a==c){
            printf("Isosceles Triangle.");
        }else{  
            printf("Scalene Triangle.");  
        }
        
    }else{
        printf("Invalid inout! Please, try again");
    }
    
    
 return 0;   
}
