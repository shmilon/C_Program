#include <stdio.h>
int main()
{
    int i,even_sum=0,odd_sum=0,n=20;
    
    for(i=1;i<=20;i++){
        
        if(i%2==0){
            even_sum=even_sum+i;
        }else{
            odd_sum=odd_sum+i;
        }
        
    }

    if(even_sum>odd_sum){
        printf("Even Is Winner");
    }else{
        printf("Odd is Winner");
    }


    return 0;
}

